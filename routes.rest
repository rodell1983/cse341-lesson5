POST http://localhost:8080/mission HTTP/1.1
content-type: application/json
  {
    "name": "Crew Dragon Demo-2",
    "launchSite": "Cape Canaveral",
    "vehicle": "Falcon 9",
    "duration": "63d 23h 25m",
    "crew": ["Behnken","Hurley"],
    "apogee": "262",
    "destination": "Space Station"
  }



